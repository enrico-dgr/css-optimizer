    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@enrico-dgr/fp-ts-fs": "^1.0.3",
    "@enrico-dgr/fp-ts-ssi": "^1.0.1",
